datasource test {
  provider = "mysql"
  url = env("TEST_DATABASE_URL")
}

model A {
  id        Int      @id
}
model B {
  id        Int      @id
}
model C {
  id            Int     @id
  foreignTable  $tables(A, B)
  foreignId     Int
  foreign       A | B       @relation(fields: [foreignId], references: [id])
}
